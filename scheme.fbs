
namespace ftunnel;

table Packet {
  version:ushort;     // 节点版本号
  conn_id:ulong;      // 连接ID，由监听角色（Listener）节点根据本机信息和时间生成
  packet_id:ushort;   // 数据包ID，递增
  dest_node:ulong;    // 数据包目的地节点：监听角色（Listener）节点或指定的连接角色（Connector）节点，或节点组别。
  src_node:ulong;     // 数据包发起者节点ID
  route_list:[ulong]; // 已经经过的节点列表（包括本节点）
  properties:ushort;  // 数据包属性：是否加密，是上行包还是下行包，是否压缩
  content:[ubyte];    // 数据包数据内容
}

root_type Packet;

